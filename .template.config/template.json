{
  "$schema": "https://json.schemastore.org/template",
  "author": "Andy Miguel Reyes R",
  "classifications": ["web", "api", "aspnet"],
  "identity": "Plantilla-Microservicio.Template",
  "name": "Plantilla-Microservicio Project Template",
  "shortName": "intraser-template",
  "tags": {
    "language": "C#",
    "type": "project"
  },
  "sourceName": "PlantillaMicroServicio",
  "defaultName": "IntranetServices",
  "symbols": {
    "UoWInterfaceName": {
      "type": "generated",
      "generator": "join",
      "parameters": {
        "separator": "",
        "parts": [
          "I",
          "name",
          "UoW"
        ]
      },
      "replaces": "__UoWInterfaceName__",
      "description": "Genera el nombre de la interfaz de Unidad de Trabajo (Unit of Work)."
    },
    "UoWClassName": {
      "type": "generated",
      "generator": "join",
      "parameters": {
        "separator": "",
        "parts": [
          "name",
          "UoW"
        ]
      },
      "replaces": "__UoWClassName__",
      "description": "Genera el nombre de la clase de Unidad de Trabajo (Unit of Work)."
    },
    "DbContextClassName": {
      "type": "generated",
      "generator": "join",
      "parameters": {
        "separator": "",
        "parts": [
          "Context",
          "name"
        ]
      },
      "replaces": "__DbContextClassName__",
      "description": "Genera el nombre de la clase de contexto de base de datos (DbContext)."
    }
  },
  "sources": [
    {
      "modifiers": [
        {
          "exclude": [
            "**/.git/**",
            "**/bin/**",
            "**/obj/**",
            "**/.vs/**"
          ]
        }
      ]
    }
  ]
}
